# Development Environment Configuration
# Copy this to .env for local development

# Required: OpenAI-compatible backend configuration
OPENAI_API_BASE_URL=http://localhost:8000/v1
OPENAI_API_KEY=your-dev-api-key

# Server configuration
PROXY_PORT=11434
DEBUG=true

# Logging
LOG_LEVEL=DEBUG

# Performance settings for development
REQUEST_TIMEOUT=30
MAX_RETRIES=1

# Model mapping (optional)
MODEL_MAPPING_FILE=./examples/config/model_mapping.json

# Development features (future)
# ENABLE_METRICS=true
# METRICS_PORT=9090