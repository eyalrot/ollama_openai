# OpenRouter Configuration
# For testing with OpenRouter's free models

# OpenRouter API configuration
OPENAI_API_BASE_URL=https://openrouter.ai/api/v1
OPENAI_API_KEY=sk-or-v1-your-openrouter-api-key-here

# Server configuration
PROXY_PORT=11434
DEBUG=false

# Logging
LOG_LEVEL=INFO

# OpenRouter can be slower, increase timeouts
REQUEST_TIMEOUT=120
MAX_RETRIES=3

# Model mapping for OpenRouter models
# Note: OpenRouter uses different model names
MODEL_MAPPING_FILE=./examples/config/openrouter_models.json